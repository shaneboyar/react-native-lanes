{
    "env": {
        "es6": true
    },
    "extends": [
        "airbnb",
        "prettier"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "parser": "babel-eslint",
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "import/no-unresolved": "off",
        "no-unused-expressions": "warn",
        "react/jsx-filename-extension": "off",
        "no-underscore-dangle": ["error", { "allowAfterThis": true }],
        "prettier/prettier": ["error"]
    }
}